int inc() 
{
    int i = 0, j = 0, k;

    /* 
    initialization
    work
    */
    while (i <= 10) {
        a[i][1] = a[i][i] = 1;
        i = i + 1;
        k = k + a[i][1] + a[i][i];
    }

    //  pascal triangle calculation
    i = 2;
    k = 0;
    while (i <= 10) {
        j = 2;
        while (j < i) {
            a[i][j] = a[i-1][j] + a[i-1][j-1];
            k = k + a[i][j];
            j = j + 2;
        }
        i = i + 2;
    }    
}